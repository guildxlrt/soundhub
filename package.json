{
	"name": "@soundhub/source",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"format": "nx format:write && prisma format",
		"lint": "nx run-many -t lint --projects=tag:scope:api",
		"prepare": "husky install",
		"build": "nx run-many -t build --projects=tag:scope:api",
		"serve": "nx run server:serve",
		"graph": "nx reset && nx graph",
		"migrate": "prisma migrate dev --name init"
	},
	"workspaces": [
		"apps/*",
		"libs/*"
	],
	"private": true,
	"dependencies": {
		"@prisma/client": "5.7.1",
		"axios": "^1.0.0",
		"dotenv": "^16.3.1",
		"express": "~4.18.1",
		"tslib": "^2.3.0",
		"validator": "^13.11.0"
	},
	"devDependencies": {
		"@nx/esbuild": "v17.2.5",
		"@nx/eslint": "v17.2.5",
		"@nx/eslint-plugin": "17.2.5",
		"@nx/jest": "v17.2.5",
		"@nx/js": "v17.2.5",
		"@nx/node": "v17.2.5",
		"@nx/workspace": "v17.2.5",
		"@swc-node/register": "~1.6.7",
		"@swc/core": "~1.3.85",
		"@types/express": "~4.17.13",
		"@types/jest": "^29.4.0",
		"@types/node": "18.16.9",
		"@types/validator": "^13.11.7",
