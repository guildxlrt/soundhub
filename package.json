		"husky": "^8.0.3",
		"jest": "^29.4.1",
		"jest-environment-node": "^29.4.1",
		"lerna": "^8.0.1",
		"nx": "v17.2.5",
		"prettier": "^2.8.8",
		"prisma": "^5.7.1",
		"ts-jest": "^29.1.0",
		"ts-node": "10.9.1",
		"typescript": "~5.2.2"
	}
